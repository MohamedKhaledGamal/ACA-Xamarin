<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ACA.Views.Survey.SurveyQuestions"
             BackgroundColor="White"
             BackgroundImage="homebg.png">
    <ContentPage.ToolbarItems>
        <ToolbarItem Icon="saveicon.png" Clicked="Save_Clicked" Order="Primary" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <StackLayout x:Name="QuestionForm" VerticalOptions="EndAndExpand" Padding="30,0,0,0">
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>